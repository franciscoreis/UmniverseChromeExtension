
{
  "name": "Umniverse - 3D tab manager",
  "description": "2D <--> 3D = bridging worlds",
  "version": "0.1.40",
  "manifest_version": 3,
  "background": 
    {
    "service_worker": "background.js"
    },
  "action": {
    "default_popup": "popup.html",
	"default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": 
  {
	"16": "/images/get_started16.png",
	"32": "/images/get_started32.png",
	"48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
"permissions": ["activeTab"
				, "scripting"
				, "pageCapture"
				, "storage"
				],
"host_permissions": [
					 "https://umniverse.com/*"
					,"http://localhost:8080/*"
					],
"externally_connectable": 
  {
	"ids": [
      "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
		],
    "matches":[
		 	   "https://umniverse.com/*"
		 	  ,"https://*.umniverse.com/*"
			  ,"http://localhost:8080/*"
			  ]
  }
				
}